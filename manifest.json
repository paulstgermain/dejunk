{
  "name": "Dejunk",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup/test.html",
    "default_icon": "images/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.quora.com/*", "https://www.quora.com/*"],
      "js": ["scripts/content.js"]
    }
  ]
}